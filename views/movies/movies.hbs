<div class="row">
  {{#each movies}}
    <div class="container">
      <h3>{{title}}</h3>
      <p>Genre:{{genre}}</p>
      <p>Plot:{{plot}}</p>
      <p>{{cast}}</p>
      <div class="buttom">
        <a href="/movies/{{_id}}/edit">Edit</a>
        <form action="/movies/{{_id}}/delete" method="post">
          <button>Delete</button>
        </form>
      </div>
    </div>
  {{/each}}
</div>

